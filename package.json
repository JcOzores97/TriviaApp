{
	"name": "triviaapp",
	"version": "0.0.0",
	"private": true,
	"scripts": {
		"server": "node ./bin/www",
		"client": "cd client && npm run start ",
		"con-start": "concurrently \"npm run server\" \"npm run client\"",
		"install-client": "cd client && npm install",
		"build": "cd client && npm run build",
		"heroku-postbuild": "npm run install-client && npm run build"
	},
	"dependencies": {
		"btoa": "^1.2.1",
		"concurrently": "^5.3.0",
		"cookie-parser": "~1.4.4",
		"debug": "~2.6.9",
		"dotenv": "^8.2.0",
		"express": "~4.16.1",
		"node-fetch": "^2.6.1",
		"socket.io": "^4.1.3"
	},
	"devDependencies": {
		"nodemon": "^2.0.4"
	},
	"engines": {
		"node": "12.16.1"
	}
}
